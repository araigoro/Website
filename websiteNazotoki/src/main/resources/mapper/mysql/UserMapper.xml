<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--Mapperとxmlのマッピング -->
<mapper namespace="com.example.demo.repository.UserMapper">

	<!--ユーザを1件登録 -->
	<insert id="insertUser">
		insert into user(

		name,
		password

		)values(

		#{userName}
		#{password}

		)

	</insert>

</mapper>